# Cache and logs (Symfony2)
/app/cache/*
/app/logs/*
!app/cache/.gitkeep
!app/logs/.gitkeep

# Email spool folder
/app/spool/*

# Cache, session files and logs (Symfony3)
/var/cache/*
/var/logs/*
/var/sessions/*
!var/cache/.gitkeep
!var/logs/.gitkeep
!var/sessions/.gitkeep

# Logs (Symfony4)
/var/log/*
!var/log/.gitkeep

# Parameters
/app/config/parameters.yml
/app/config/parameters.ini

# Managed by Composer
/app/bootstrap.php.cache
/var/bootstrap.php.cache
/bin/*
!bin/console
!bin/symfony_requirements
/vendor/

# Assets and user uploads
/web/bundles/
/web/uploads/

# PHPUnit
/app/phpunit.xml
/phpunit.xml

# Build data
/build/

# Composer PHAR
/composer.phar

# Backup entities generated with doctrine:generate:entities command
**/Entity/*~

# Embedded web-server pid file
/.web-server-pid

# Ignore sensitive or temporary files and directories
/.env
/.env.local
/.env.*.local
/.env.test.local
/.env.test

# Symfony-related directories
/var/
/public/bundles/
/translations/*.yaml
/.phpunit.result.cache
/node_modules/
/composer.lock

# Docker-specific ignores
/docker-compose.override.yml
/.dockerignore
/vendor
/var
/public
/node_modules

# Ignore IDE-specific files and directories
/.idea/
/.vscode/

# Ignore MacOS-specific files
.DS_Store

# Ignore other OS generated files
Thumbs.db
Desktop.ini

# Ignore PHPStorm-specific files
/.phpstorm.meta.php

# Ignore files related to the Symfony Encore build (webpack)
/public/build/
/public/assets/

# Ignore local configuration files
/.env.local.php

# Ignore Docker container and data directories
/docker/

# Ignore composer and npm logs
/npm-debug.log
/yarn-error.log

# Ignore miscellaneous binaries
/bin/
/sbin/

# Ignore miscellaneous user-specific files
*.swp
*.swo

